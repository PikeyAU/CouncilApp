<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReportTracking">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="403dp"
        android:layout_height="110dp"
        android:layout_marginBottom="400dp"
        android:contentDescription="@string/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/Username"
        android:layout_width="130dp"
        android:layout_height="36dp"
        android:layout_marginTop="109dp"
        android:text="@string/username"
        android:gravity="center"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/logo" />

    <TextView
        android:id="@+id/title"
        android:layout_width="356dp"
        android:layout_height="32dp"
        android:layout_marginTop="20dp"
        android:background="#FFFFFF"
        android:text="@string/track_your_reports"
        android:textAlignment="center"
        android:gravity="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Username" />

    <TableLayout
        android:layout_width="414dp"
        android:layout_height="369dp"
        android:layout_marginTop="20dp"
        android:collapseColumns="0.5
"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="33dp">

            <TextView
                android:id="@+id/LocationColumn"
                android:layout_width="197dp"
                android:layout_height="63dp"
                android:background="@drawable/border"
                android:gravity="center"
                android:text="@string/location"
                android:textAlignment="center"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/TimeColumn"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:background="@drawable/border"
                android:gravity="center"
                android:text="@string/time"
                android:textAlignment="center"
                android:textSize="16sp"
                android:textStyle="bold"
                android:visibility="visible" />

            <TextView
                android:id="@+id/SituationColumn"
                android:layout_width="73dp"
                android:layout_height="match_parent"
                android:background="@drawable/border"
                android:text="@string/situation"
                android:gravity="center"
                android:textAlignment="center"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/FeedbackColumn"
                android:layout_width="76dp"
                android:layout_height="match_parent"
                android:background="@drawable/border"
                android:text="@string/feedback"
                android:gravity="center"
                android:textAlignment="center"
                android:textSize="16sp"
                android:textStyle="bold" />
        </TableRow>

        <TableRow
